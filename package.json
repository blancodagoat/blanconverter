{
  "name": "file-converter",
  "version": "1.0.0",
  "description": "A free online file conversion tool for everyone",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm run build-css && npm run build-js",
    "build-css": "sass src/styles/main.scss public/css/style.css --style compressed",
    "build-js": "webpack --mode production",
    "watch": "sass src/styles/main.scss public/css/style.css --watch",
    "vercel-build": "npm run build"
  },
  "keywords": [
    "file-converter",
    "online-tools",
    "free-converter"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "7zip-bin": "^5.1.1",
    "archiver": "^7.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "docx": "^9.5.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "fluent-ffmpeg": "^2.1.3",
    "fontkit": "^2.0.2",
    "helmet": "^8.1.0",
    "js-yaml": "^4.1.0",
    "libreoffice-convert": "^1.4.1",
    "mammoth": "^1.6.0",
    "multer": "^1.4.5-lts.1",
    "node-7z": "^2.1.2",
    "officegen": "^0.6.5",
    "opentype.js": "^1.3.4",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "pdf2pic": "^3.1.0",
    "sharp": "^0.34.3",
    "svg2pdf.js": "^2.2.4",
    "svgo": "^4.0.0",
    "tar": "^7.4.3",
    "three": "^0.179.1",
    "unzipper": "^0.10.14",
    "xlsx": "^0.18.5",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "babel-loader": "^10.0.0",
    "nodemon": "^3.0.2",
    "sass": "^1.69.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^6.0.1"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
