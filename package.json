{
  "name": "file-converter",
  "version": "1.0.0",
  "description": "A free online file conversion tool for everyone",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm run build-css && npm run build-js",
    "build-css": "sass src/styles/main.scss public/css/style.css --style compressed",
    "build-js": "webpack --mode production",
    "watch": "sass src/styles/main.scss public/css/style.css --watch",
    "test": "node test-conversions.js",
    "test-images": "node test-image-conversions.js",
    "test-audio": "node test-audio-conversions.js",
    "test-video": "node test-video-conversions.js",
    "test-ebooks": "node test-ebook-conversions.js",
    "test-archives": "node test-archive-conversions.js",
    "test-fonts": "node test-font-conversions.js",
    "test-cad": "node test-cad-conversions.js",
    "test-vectors": "node test-vector-conversions.js",
    "test-data": "node test-data-conversions.js",
    "test-disk-images": "node test-disk-image-conversions.js",
    "test-specialized": "node test-specialized-conversions.js"
  },
  "keywords": ["file-converter", "online-tools", "free-converter"],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "pdf-lib": "^1.17.1",
    "docx": "^8.5.0",
    "xlsx": "^0.18.5",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "mammoth": "^1.6.0",
    "pdf2pic": "^3.1.0",
    "officegen": "^0.6.5",
    "libreoffice-convert": "^1.4.1",
    "pdf-parse": "^1.1.1",
    "svg2pdf.js": "^2.2.4",
    "archiver": "^6.0.1",
    "unzipper": "^0.10.14",
    "tar": "^6.2.0",
    "7zip-bin": "^5.1.1",
    "node-7z": "^2.1.2",
    "opentype.js": "^1.3.4",
    "fontkit": "^2.0.2",
    "three": "^0.179.1",
    "svgo": "^3.0.2",
    "js-yaml": "^4.1.0",
    "xml2js": "^0.6.2",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "sass": "^1.69.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "babel-loader": "^9.1.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
} 